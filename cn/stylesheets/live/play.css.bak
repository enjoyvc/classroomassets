.pop-employ .title{padding: 10px 20px 10px 10px;border-bottom: 1px solid #292929;overflow: hidden;}
.pop-employ .prv-con{padding: 20px 10px 0 20px;border-top: 1px solid #3f3f3f;}
.pop-employ .close{width: 16px;height: 16px;margin-top:5px;background: url(../../images/icon_cose_spec.png) no-repeat;}
.pop-employ .title .nav{color:#666;}
.pop-employ .title .nav span{display:inline-block;font:500 16px/32px 'Microsoft YaHei';padding:0 10px;margin-right: 3px;cursor: pointer;}
.pop-employ .title .nav span.on{color:#fff;background: #464646;border-radius: 7px;}
.pop-employ .prv-box{display: none;}
.pop-employ .prv-box.on{display: block;}
.pop-employ .choose{overflow: hidden;}
.pop-employ .choose span{display:inline-block;font:500 14px/30px 'Microsoft YaHei';height: 30px;padding:0 15px;margin:0 5px;background: #292929;border-radius: 7px;cursor: pointer;}
.pop-employ .choose span.on{background: #464646;color: #fff;}
.pop-employ .btn-save{display:inline-block;width:110px;font:500 14px/30px 'Microsoft YaHei';height:36px;margin:25px auto auto auto;color:#fff;border:1px solid #292929;background: #505050;border-radius: 7px;text-align: center;text-decoration: none;cursor: pointer;}
.pop-employ .file-con {margin-top:18px;}
.pop-employ .file-con input{opacity: 0;width: 100%;position: absolute;top: 0;left: 0;}
.pop-employ .file-con .lab{display:inline-block;height:25px;line-height:25px;width: 100%;padding:0 8px;margin:5px 0;background: #292929;border-radius: 7px;font-size: 12px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;box-sizing: border-box;}
.pop-employ .file-con  h2{font:500 16px/30px 'Microsoft YaHei';margin-bottom:6px;}
.pop-employ .file-con-b{margin-top:10px;}
.pop-reding{position: absolute;bottom:30px;right: 72px;color: #fff;}
.pop-reding .sta{padding:10px 20px;background: #333;border-radius: 7px;overflow: hidden;margin:8px 0;height:30px;overflow: hidden;}
.pop-reding .close{width: 16px;height: 16px;margin-top:7px;margin-left:18px;background: url(../../images/icon_cose_spec.png) no-repeat;}
.pop-reding .inner{display: inline-block;height:30px;font-family: 'Microsoft YaHei';font-size: 14px;line-height: 30px;color:#666;padding-left: 14px;vertical-align: top;overflow: hidden;}
.pop-reding .time{display: inline-block;width: 100px;height: 30px;font: 500 18px/30px Arial;color: #a2a2a2;padding-left: 10px;overflow: hidden;}
.pop-reding .icon-a{display: block;width:30px;height: 30px;background: url(../../images/icon_reding.png) no-repeat;cursor: pointer;}
.pop-reding .icon-b{display: block;width:30px;height: 30px;background: url(../../images/icon_reding.png) no-repeat;background-position:0 -61px;cursor: pointer;}
.pop-reding .inner .lab{display: inline-block;height: 30px;font: 500 12px/30px Arial;vertical-align: top;}
.pop-reding .icon-a:hover,.pop-reding .icon-a.on{background-position: 0 -184px;}
.pop-reding .icon-b:hover,.pop-reding .icon-b.on{background-position: 0 -119px;}